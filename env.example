# CodeGym AI Environment Variables
# Copy this file to .env.local and fill in your actual values

# Required: Google Gemini API Key
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_AI_API_KEY=your_gemini_api_key_here

# Optional: App URL (auto-detected in development)
NEXT_PUBLIC_APP_URL=http://localhost:9002

# Optional: Environment
NODE_ENV=development

# Optional: Database (for future use)
# DATABASE_URL=postgresql://username:password@localhost:5432/codegym_ai

# Optional: Authentication (for future use)
# NEXTAUTH_SECRET=your_nextauth_secret_here
# NEXTAUTH_URL=http://localhost:9002

# Optional: Monitoring (for future use)
# SENTRY_DSN=your_sentry_dsn_here
# GOOGLE_ANALYTICS_ID=your_ga_id_here
